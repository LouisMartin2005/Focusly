{
  "manifest_version": 3,
  "name": "Focusly",
  "version": "1.0",
  "description": "Professional ADHD Focus Mode Chrome Extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": { "16": "logo16.png", "48": "logo48.png", "128": "logo128.png" }
  },
  "permissions": ["storage", "tabs", "webNavigation"],
  "background": { "service_worker": "background.js" },
  "icons": { "16": "logo16.png", "48": "logo48.png", "128": "logo128.png" }
}
